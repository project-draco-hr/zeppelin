{
  super.setInterpreterGroup(interpreterGroup);
synchronized (interpreterGroupReference) {
    if (interpreterGroupReference.containsKey(getInterpreterGroupKey(interpreterGroup))) {
      interpreterGroupReference.remove(getInterpreterGroupKey(interpreterGroup));
    }
    interpreterGroupReference.put(getInterpreterGroupKey(interpreterGroup),new RemoteInterpreterProcess(interpreterRunner,interpreterPath,env,interpreterContextRunnerPool));
    logger.info("setInterpreterGroup = " + getInterpreterGroupKey(interpreterGroup) + " class="+ className+ ", path="+ interpreterPath);
  }
}
