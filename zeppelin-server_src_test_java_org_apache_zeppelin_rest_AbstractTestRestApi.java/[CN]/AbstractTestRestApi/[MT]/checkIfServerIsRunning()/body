{
  GetMethod request=null;
  boolean isRunning=true;
  try {
    request=httpGet("/");
    isRunning=request.getStatusCode() == 200;
  }
 catch (  IOException e) {
    LOG.error("Exception in AbstractTestRestApi while checkIfServerIsRunning ",e);
    isRunning=false;
  }
 finally {
    if (request != null) {
      request.releaseConnection();
    }
  }
  return isRunning;
}
