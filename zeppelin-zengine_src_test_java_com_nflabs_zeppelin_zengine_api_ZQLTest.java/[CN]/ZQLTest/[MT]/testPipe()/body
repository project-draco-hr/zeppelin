{
  ZQL zql=new ZQL();
  zql.append("select * from bank | select * from <%= z." + Q.INPUT_VAR_NAME + " %> limit 10");
  ZPlan plan=zql.compile();
  assertEquals(1,plan.size());
  assertEquals("select * from " + plan.get(0).prev().name() + " limit 10",plan.get(0).getQuery());
  plan.get(0).release();
}
