{
  printWelcomeMessage();
  ConsoleReader reader=new ConsoleReader();
  reader.setBellEnabled(false);
  String line;
  while ((line=readLine(reader,"")) != null) {
    ZQL zql;
    try {
      zql=new ZQL(line);
      LinkedList<Result> rs=zql.compile().execute(z);
      for (      Result r : rs) {
        r.write(System.out);
      }
    }
 catch (    ZException e) {
      e.printStackTrace();
    }
catch (    ZQLException e) {
      e.printStackTrace();
    }
catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
