{
  if (paragraph == null)   return null;
  String magic=null;
  for (int i=0; i < paragraph.length(); i++) {
    int ch=paragraph.charAt(i);
    if (!(i == 0 && ch == '%')) {
      break;
    }
 else {
      magic="";
    }
    if (ch == ' ' || ch == '\n') {
      break;
    }
 else {
      magic+=ch;
    }
  }
  return magic;
}
