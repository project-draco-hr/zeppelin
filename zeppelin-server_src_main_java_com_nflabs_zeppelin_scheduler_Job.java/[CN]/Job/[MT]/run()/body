{
  if (aborted) {
    setStatus(Status.ABORT);
    return;
  }
  try {
    setStatus(Status.RUNNING);
    dateStarted=new Date();
    result=jobRun();
    dateFinished=new Date();
    if (aborted) {
      setStatus(Status.ABORT);
    }
 else {
      setStatus(Status.FINISHED);
    }
  }
 catch (  Throwable e) {
    this.exception=e;
    dateFinished=new Date();
    setStatus(Status.ERROR);
  }
}
