{
  String tableName=genTableName();
  String tc="CREATE EXTERNAL TABLE " + tableName + "("+ schema.toHiveTableCreationQueryColumnPart()+ ") "+ "ROW FORMAT DELIMITED FIELDS TERMINATED BY '"+ split+ "' "+ "STORED AS TEXTFILE "+ "LOCATION '"+ location.toString()+ "'";
  try {
    sharkContext.sql(tc,0);
  }
 catch (  Exception e) {
    throw new ZeppelinRuntimeException(e);
  }
  return fromTable(tableName);
}
