{
  TTupleProtocol oprot=(TTupleProtocol)prot;
  BitSet optionals=new BitSet();
  if (struct.isSetCode()) {
    optionals.set(0);
  }
  if (struct.isSetMsg()) {
    optionals.set(1);
  }
  oprot.writeBitSet(optionals,2);
  if (struct.isSetCode()) {
    oprot.writeString(struct.code);
  }
  if (struct.isSetMsg()) {
    oprot.writeString(struct.msg);
  }
}
