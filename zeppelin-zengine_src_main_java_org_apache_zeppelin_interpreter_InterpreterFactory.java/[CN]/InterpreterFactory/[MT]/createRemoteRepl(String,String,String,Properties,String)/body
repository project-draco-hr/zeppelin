{
  int connectTimeout=conf.getInt(ConfVars.ZEPPELIN_INTERPRETER_CONNECT_TIMEOUT);
  String localRepoPath=conf.getInterpreterLocalRepoPath() + "/" + interpreterSettingId;
  int maxPoolSize=conf.getInt(ConfVars.ZEPPELIN_INTERPRETER_MAX_POOL_SIZE);
  updatePropertiesFromRegisteredInterpreter(property,className);
  LazyOpenInterpreter intp=new LazyOpenInterpreter(new RemoteInterpreter(property,noteId,className,conf.getInterpreterRemoteRunnerPath(),interpreterPath,localRepoPath,connectTimeout,maxPoolSize,remoteInterpreterProcessListener));
  return intp;
}
