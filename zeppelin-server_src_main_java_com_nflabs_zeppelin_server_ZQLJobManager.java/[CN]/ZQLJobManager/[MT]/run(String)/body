{
  ZQLJob s=get(jobId);
  if (s == null) {
    return null;
  }
  if (s.getStatus() == Status.PENDING || s.getStatus() == Status.RUNNING) {
    return s;
  }
 else {
    s.setStatus(Status.READY);
  }
  s.setListener(this);
  logger.info("Job " + jobId + " sumitted to run");
  scheduler.submit(s);
  return s;
}
