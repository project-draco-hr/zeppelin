{
  ZeppelinITUtils.sleep(500,false);
  driver.findElement(By.xpath("//div[contains(@class, 'navbar-collapse')]//li[contains(.,'" + userName + "')]")).click();
  ZeppelinITUtils.sleep(500,false);
  driver.findElement(By.xpath("//div[contains(@class, 'navbar-collapse')]//li[contains(.,'" + userName + "')]//a[@ng-click='logout()']")).click();
  ZeppelinITUtils.sleep(5000,false);
}
