{
synchronized (active) {
    ZQLSession session=active.get(sessionId);
    if (session != null)     return session;
  }
  Path path=getPathForSessionId(sessionId);
  try {
    ZQLSession session=load(path);
    if (session != null) {
      return session;
    }
 else {
      return null;
    }
  }
 catch (  IOException e) {
    return null;
  }
}
