{
  Param param=null;
  for (  Param p : params) {
    if (name.equals(p.getName())) {
      param=p;
      break;
    }
  }
  if (param == null)   return null;
  for (  ParamSpec ps : getParamSpec()) {
    if (name.equals(ps.getName())) {
      return ps.validate(param.getValue());
    }
  }
  return param.getValue();
}
