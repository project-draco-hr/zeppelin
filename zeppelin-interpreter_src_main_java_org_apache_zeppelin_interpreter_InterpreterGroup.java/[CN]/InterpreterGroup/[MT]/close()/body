{
  List<Thread> closeThreads=new LinkedList<Thread>();
  for (  final Interpreter intp : this) {
    Thread t=new Thread(){
      public void run(){
        intp.close();
      }
    }
;
    t.start();
    closeThreads.add(t);
  }
  for (  Thread t : closeThreads) {
    try {
      t.join();
    }
 catch (    InterruptedException e) {
      LOGGER.error("Can't close interpreter",e);
    }
  }
}
