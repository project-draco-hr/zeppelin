{
  ResponseBuilder r=javax.ws.rs.core.Response.status(status).entity(this.toString());
  if (cookies != null) {
    for (    NewCookie nc : cookies) {
      r.cookie(nc);
    }
  }
  r.header("Access-Control-Allow-Origin","*").header("Access-Control-Allow-Credentials","true").header("Access-Control-Allow-Headers","authorization,Content-Type").header("Access-Control-Allow-Methods","POST, GET, OPTIONS , PUT, HEAD, DELETE");
  return r.build();
}
