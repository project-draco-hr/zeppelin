{
synchronized (this) {
    String[] stmts=st.split("\n");
    sc.setJobGroup(jobGroup,"Zeppelin",false);
    Result r=null;
    for (    String s : stmts) {
      r=getResult(intp.interpret(s));
      if (r == Result.ERROR) {
        sc.clearJobGroup();
        return r;
      }
    }
    sc.clearJobGroup();
    return r;
  }
}
