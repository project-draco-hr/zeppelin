{
  LinkedList<Result> results=new LinkedList<Result>();
  ZQL zqlEvaluator=new ZQL(zql);
  List<Z> zqlResult;
  zqlResult=zqlEvaluator.compile();
  for (  Z zz : zqlResult) {
    try {
      results.add(zz.execute().result());
    }
 catch (    ZException e) {
      error=new Result(e);
      throw e;
    }
  }
  return results;
}
