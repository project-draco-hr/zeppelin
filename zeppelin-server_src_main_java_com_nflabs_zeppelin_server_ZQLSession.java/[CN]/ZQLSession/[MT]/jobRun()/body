{
  LinkedList<Result> results=new LinkedList<Result>();
  if (zqlPlans == null) {
    ZQL zqlEvaluator=new ZQL(zql);
    zqlPlans=zqlEvaluator.compile();
  }
  for (  Z zz : zqlPlans) {
    try {
      if (zz.isExecuted() == false) {
        zz.execute();
      }
      results.add(zz.result());
    }
 catch (    ZException e) {
      error=new Result(e);
      throw e;
    }
  }
  return results;
}
