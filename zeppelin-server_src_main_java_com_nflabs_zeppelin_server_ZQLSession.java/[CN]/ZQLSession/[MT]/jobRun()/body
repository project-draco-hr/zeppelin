{
  LinkedList<ResultDataObject> results=new LinkedList<ResultDataObject>();
  ZQL zqlEvaluator=new ZQL(zql);
  List<Z> zqlResult;
  zqlResult=zqlEvaluator.compile();
  for (  Z zz : zqlResult) {
    List<ResultSet> res;
    try {
      res=zz.execute();
      for (      ResultSet r : res) {
        results.add(new ResultDataObject(r));
      }
    }
 catch (    ZException e) {
      error=new ResultDataObject(e);
      throw e;
    }
  }
  return results;
}
