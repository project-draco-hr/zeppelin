{
  LinkedList<ResultDataObject> results=new LinkedList<ResultDataObject>();
  ZQL zqlEvaluator=new ZQL(zql);
  List<Z> zqlResult;
  zqlResult=zqlEvaluator.compile();
  for (  Z zz : zqlResult) {
    ResultSet res;
    try {
      res=zz.execute();
      results.add(new ResultDataObject(res));
    }
 catch (    ZException e) {
      error=new ResultDataObject(e);
      throw e;
    }
  }
  return results;
}
