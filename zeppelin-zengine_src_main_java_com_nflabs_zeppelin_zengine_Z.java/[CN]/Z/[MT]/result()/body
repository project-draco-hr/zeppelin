{
  if (executed == false) {
    throw new ZException("Can not get result because of this is not executed");
  }
  if (result == null) {
    if (name() == null) {
      if (lastQueryResult != null) {
        result=lastQueryResult;
      }
 else {
        if (prev() != null) {
          result=prev().result();
        }
      }
    }
 else {
      try {
        result=connection().select(name(),maxResult);
      }
 catch (      Exception e) {
        if (lastQueryResult != null) {
          result=lastQueryResult;
        }
      }
    }
  }
  return result;
}
