{
  try {
    Result desc=new Result(executeQuery("describe " + name),10000);
    desc.load();
    if (desc.getRows().size() < 5)     return false;
    if (((String)desc.getRows().get(0)[0]).contains("not exist"))     return false;
    return true;
  }
 catch (  ZException e) {
    return false;
  }
catch (  ResultDataException e) {
    return false;
  }
catch (  SQLException e) {
    return false;
  }
}
