{
  Z z1=new Q("select * from bank",z);
  Z z2=new Q("select * from (<%=z." + Q.INPUT_VAR_NAME + "%>) q limit 10",z);
  assertNull(z1.name());
  z1.pipe(z2);
  assertNull(z2.name());
  assertNotNull(z1.name());
  z1.unPipe();
  assertNull(z1.name());
  assertNull(z2.name());
  z1.withName("hello");
  z1.pipe(z2);
  z1.unPipe();
  assertEquals("hello",z1.name());
}
