{
  Z q=new Q("select * from bank",z,drv).pipe(new Q("select * from (<%=z." + Q.INPUT_VAR_NAME + "%>) q limit 10",z,drv)).pipe(new Q("create view vv as select * from <%=z." + Q.INPUT_VAR_NAME + "%>",z,drv));
  assertEquals("create view vv as select * from " + q.prev().name(),q.getQuery());
  q.release();
}
