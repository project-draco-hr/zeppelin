{
  if (text == null) {
    return null;
  }
  int scriptHeadIndex=0;
  for (int i=0; i < text.length(); i++) {
    char ch=text.charAt(i);
    if (ch == ' ' || ch == '\n' || ch == '(') {
      scriptHeadIndex=i;
      break;
    }
  }
  if (scriptHeadIndex == 0) {
    return null;
  }
  String head=text.substring(0,scriptHeadIndex);
  if (head.startsWith("%")) {
    return head.substring(1);
  }
 else {
    return null;
  }
}
