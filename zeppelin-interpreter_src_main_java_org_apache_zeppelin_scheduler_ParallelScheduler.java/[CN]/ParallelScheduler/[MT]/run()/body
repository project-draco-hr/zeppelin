{
  while (terminate == false) {
    Job job=null;
synchronized (queue) {
      if (running.size() >= maxConcurrency || queue.isEmpty() == true) {
        try {
          queue.wait(500);
        }
 catch (        InterruptedException e) {
        }
        continue;
      }
      job=queue.remove(0);
      running.add(job);
    }
    Scheduler scheduler=this;
    executor.execute(new JobRunner(scheduler,job));
  }
}
