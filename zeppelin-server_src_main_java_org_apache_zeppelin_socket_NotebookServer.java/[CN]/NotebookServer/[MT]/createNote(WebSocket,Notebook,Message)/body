{
  Note note=notebook.createNote();
  note.addParagraph();
  if (message != null) {
    String noteName=(String)message.get("name");
    if (noteName == null || noteName.isEmpty()) {
      noteName="Note " + note.getId();
    }
    note.setName(noteName);
  }
  note.persist();
  addConnectionToNote(note.id(),(NotebookSocket)conn);
  broadcastNote(note);
  broadcastNoteList();
}
