{
  if (after == Status.ERROR) {
    if (job.getException() != null) {
      LOG.error("Error",job.getException());
    }
  }
  if (job.isTerminated()) {
    LOG.info("Job {} is finished",job.getId());
    try {
      note.persist(null);
    }
 catch (    IOException e) {
      LOG.error(e.toString(),e);
    }
  }
  notebookServer.broadcastNote(note);
}
